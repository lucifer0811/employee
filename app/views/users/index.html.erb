<div class="col-md-6", float: "right">
  <%= render "form_search" %>
</div>
<table class="table table-hover" id="list_user">
  <thead class="title_table">
    <th>Stt</th>
    <th>Avatar</th>
    <th>Name</th>
    <th>Email</th>
  </thead>
  <tbody>
    <% @users.each_with_index do |user, index| %>
      <tr>
        <td><%= index + 1 %></td>
        <td><%= image_tag(user.avatar, size: "50") %></td>
        <td><%= link_to user.name, "#" %></td>
        <td><%= user.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>

